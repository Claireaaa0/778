{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "main": "src/lambda.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "serverless package",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "invoke:local": "serverless invoke local --function api",
    "logs": "serverless logs -f api",
    "remove": "serverless remove",
    "dev": "serverless offline start --reloadHandler",
    "dev:watch": "serverless offline start --reloadHandler",
    "start:local": "serverless offline start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "ingest": "node src/scripts/ingest.mjs",
    "ingest:help": "node src/scripts/ingest.mjs --help"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.865.0",
    "@aws-sdk/client-bedrock": "^3.865.0",
    "@aws-sdk/client-bedrock-agent": "^3.894.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.865.0",
    "@aws-sdk/client-bedrock-runtime": "^3.865.0",
    "@aws-sdk/client-cognito-identity": "^3.888.0",
    "@aws-sdk/client-connect": "^3.887.0",
    "@aws-sdk/client-connect-contact-lens": "^3.887.0",
    "@aws-sdk/client-connectparticipant": "^3.883.0",
    "@aws-sdk/client-dynamodb": "^3.645.0",
    "@aws-sdk/client-s3": "^3.865.0",
    "@aws-sdk/credential-providers": "^3.888.0",
    "@aws-sdk/lib-dynamodb": "^3.645.0",
    "@aws-sdk/s3-request-presigner": "^3.865.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "jsonwebtoken": "^9.0.2",
    "p-limit": "^7.1.1",
    "pdfjs-dist": "^3.11.174",
    "serverless-offline-aws-eventbridge": "^2.2.4",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.23.0",
    "serverless": "^3.39.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^13.4.0",
    "vitest": "^3.1.3"
  }
}
